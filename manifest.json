{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Automatically closes inactive tabs after a set period.",
  "permissions": ["tabs", "background", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"]
}
